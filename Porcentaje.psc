Algoritmo Porcentaje
	
	Escribir "Ingrese la cantidad de Mujeres porfavor"
	Leer mujeres
	Escribir "ingrese la cantidad de Hombres"
	leer hombres
	
	//Sumar los das mujeres y hombres
	operacion1=mujeres+hombres
	//sacar  el porcentaje de las mujeres
	valor1=mujeres*100/operacion1
	//Sacar el porcentaje de los hombres
	valor2=hombres*100/operacion1
	
	Escribir "El porcentaje de mujeres es de " valor1
	Escribir "El porcentaje de hombres es de " valor2
 	
FinAlgoritmo
